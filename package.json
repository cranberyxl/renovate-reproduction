{
  "name": "@teamexos/prince-backend",
  "version": "0.0.0",
  "main": "index.js",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "graphql": "16.8.1"
  },
  "devDependencies": {
    "@types/node": "20.11.1",
    "eslint-plugin-exos": "file:./eslint"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src",
      ".env"
    ],
    "exec": "yarn start",
    "ext": "ts"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.+(js|json|ts|vue|graphql|html|md)": "prettier --write"
  },
  "resolutions": {
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "expect": "29.7.0",
    "node-fetch": "2.7.0",
    "jest": "29.7.0",
    "interpret": "3.1.1"
  },
  "packageManager": "yarn@4.1.1"
}
